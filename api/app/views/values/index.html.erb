<h1>Values#index</h1>
<p>Find me in app/views/values/index.html.erb</p>
<table>
  <thead>
    <th>Id</th>
    <th>temp</th>
    <th>sound</th>
    <th>light</th>
  </thead>
  <tbody>
    <% @values.each do |item| %>
      <tr>
        <td> <%= item.id  %>  </td>
        <td> <%= item.try(:temp)  %>  </td>
        <td> <%= item.try(:sound)  %>  </td>
        <td> <%= item.try(:light)  %>  </td>
      </tr>
      <br />
    <% end %>   
  </tbody>
</table>

